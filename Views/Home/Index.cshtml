
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Home/Shared/_HomeLayout.cshtml";
}
@model List<AracKiralama.Models.Arac>

@foreach (var item in Model)
{
    <div class="col-md-3 col-sm-6 hero-feature">
        <div class="thumbnail">
            <img src="@item.imagePath" alt="">
            <div class="caption">
                <h3>@item.Marka</h3>
                <p>@item.Model @item.Plaka @item.Vites @item.Fiyat</p>
                <p>
                    <a href="#" class="btn btn-primary">Buy Now!</a> <a href="#" class="btn btn-default">More Info</a>
                </p>
            </div>
        </div>
    </div>
}
    <!-- Page Features -->
    

