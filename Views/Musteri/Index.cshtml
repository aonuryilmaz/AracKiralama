
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Home/Shared/_HomeLayout.cshtml";
}
@model List<AracKiralama.Models.Arac>
<div id="iyzipay-checkout-form" class="responsive"></div>
@foreach (var item in Model)
{
    <div class="col-md-3 col-sm-6 hero-feature">
        <div class="thumbnail">
                <img src="@item.imagePath" alt="">
            <div class="caption" >
                <h3>@item.Marka</h3>
                <span>@item.Model</span>
                <span>@item.Plaka</span>
                <span>@item.Vites</span>
                <span>@item.Fiyat&#8378;</span>
            </div>
        </div>
    </div>
}



